<!DOCTYPE html>
<html>

<head>
    <meta charset="UTF-8" />
    <title>MA2E | {% block title %}{% endblock %}</title>

    <meta name="viewport" content="width=device-width, initial-scale=1.0" />

    <link rel="stylesheet" type="text/css" href="http://fonts.googleapis.com/css?family=Signika:300,400,600,700" />
    <link rel="stylesheet" type="text/css" href="{{asset('_assets/_libs/font-awesome/css/font-awesome.css')}}" />
    <link rel="stylesheet" type="text/css" href="{{asset('_assets/_libs/no-ui-slider/jquery.nouislider.css')}}" />
    <link rel="stylesheet" type="text/css" href="{{asset('_assets/_libs/animate.css')}}" />
    <link rel="stylesheet" type="text/css" href="{{asset('_assets/_css/style.css')}}" /> {% block stylesheets %}{% endblock %}
    <link rel="icon" type="image/x-icon" href="{{ asset('_assets/_img/logo.png') }}" />
</head>

<body>

    <!--div id="loader">

        <div class="loader-spinner">
        </div>

    </div-->

    <header>

        <div class="center">

            <div class="logo">

                <a href="{{path('login')}}"><img src="{{asset('_assets/_img/logo.png')}}" alt="MA2E" /></a>

            </div>

            <nav>

                <!--ul class="menu">

                    <li><a href="#" class="menu-element" style="color: #f28931;background: #fcfdfd;padding: 10px;">Espace sécurisé <span class="fa fa-lock securise"></span> </a></li>

                </ul-->

                <div class="menu-responsive"><i class="fa fa-bars"></i></div>

            </nav>

            <div class="clear">
            </div>

        </div>

    </header>
    {% block body %}{% endblock %}


    <footer>

        <div class="center">

            <div class="green-line">
            </div>

            <ul class="right">

                <li>Copyright &copy; 2018 <span class="divider">&middot;</span> <a href="#">MA2E</a></li>


            </ul>

            <div class="clear">
            </div>

        </div>

    </footer>

    <div id="fb-root"></div>

    {% block javascripts %}
    <style>
        @import "compass/css3";
    </style>
    <script src="{{ asset('_assets/_libs/jquery.js') }}"></script>
    <!--script src="http://maps.google.com/maps/api/js?sensor=false"></script-->
    <script src="{{ asset('_assets/_libs/no-ui-slider/jquery.nouislider.all.min.js') }}"></script>
    <script src="{{ asset('_assets/_libs/smoothscroll.js') }}"></script>
    <script src="{{ asset('_assets/_libs/parallax.js') }}"></script>
    <script src="{{ asset('_assets/_js/functions.js') }}"></script>
    <script src="{{ asset('_assets/_js/customize.js') }}"></script>
    {% endblock %}
</body>

</html>